    Customer.Pesel,
    Customer.Date_Of_Birth,
    Loan.LoanID,
    Loan.Approval_Date,
    CASE 
        WHEN DATEPART(month, Loan.Approval_Date) &gt; DATEPART(month, Customer.Date_Of_Birth)
          OR (DATEPART(month, Loan.Approval_Date) = DATEPART(month, Customer.Date_Of_Birth) 
              AND DATEPART(day, Loan.Approval_Date) &gt;= DATEPART(day, Customer.Date_Of_Birth))
        THEN DATEDIFF(year, Customer.Date_Of_Birth, Loan.Approval_Date)
        ELSE DATEDIFF(year, Customer.Date_Of_Birth, Loan.Approval_Date) - 1
    END AS Age
FROM 
    Customer
INNER JOIN 
    Loan ON Customer.Pesel = Loan.CustomerID</property>
